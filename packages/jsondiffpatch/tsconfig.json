{
  "compilerOptions": {
    "target": "es6",
    "module": "node16",
    "types": [
      "node",
    ],
    "declaration": true,
    "outDir": "lib",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": false
  },
  "include": [
    "src"
  ],
}
